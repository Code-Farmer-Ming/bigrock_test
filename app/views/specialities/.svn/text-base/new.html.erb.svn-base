
  <div class="form_info">
    <h1>创建 专长信息</h1>
    <% form_for(@speciality,:url=>user_resume_specialities_path(params[:user_id],params[:resume_id]),:html=>{:method=>:post}) do |f| %>
      <%= f.error_messages %>
      <%= render :partial=>"specialities/form",:object=>f %>
      <p>
        <%= f.submit '保存并继续创建' %> <%= link_to "完成" ,user_resumes_path(params[:user_id])%>
      </p>
    <% end %>
  </div>
