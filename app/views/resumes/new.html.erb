
  <div class="form_info">
    <h1>新建档案</h1>

    <% form_for(@resume,:url=> user_resumes_path(params[:user_id]),:html=>{:method=>:post})  do |f| %>
      <%= f.error_messages %>

      <%= render :partial=>"resumes/form" ,:object=>f%>

      <p>
        <%= f.submit '保存' %> <%= f.submit '下一步' %>
      </p>
    <% end %>
  </div>
