<h1>Listing educations</h1>

<table>
  <tr>
    <th>School name</th>
    <th>Begin date</th>
    <th>End date</th>
    <th>Description</th>
  </tr>

<% @educations.each do |education| %>
  <tr>
    <td><%=h education.school_name %></td>
    <td><%=h education.begin_date %></td>
    <td><%=h education.end_date %></td>
    <td><%=h education.description %></td>
 
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New education', new_user_resume_education_path(params[:user_id],params[:resume_id]) %>