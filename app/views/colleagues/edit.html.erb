<h1>Editing colleague</h1>

<% form_for(@colleague) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :colleague_id %><br />
    <%= f.text_field :colleague_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @colleague %> |
<%= link_to 'Back', colleagues_path %>