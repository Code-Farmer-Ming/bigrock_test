<div id="left_content">
  <h1><%= @label %>  所有标签 </h1>

  <%= render :partial=>"tags/search" %>

  <div>
    <%if  params[:company_id]%>
      <%= highlight(render(:partial=>"tags/show_tag" ,:collection=>@tags,:locals=>{:type=>@tag_type}),params[:search])  %>
    <%elsif params[:user_id]%>
      <%= highlight(render(:partial=>"resumes/show_tag" ,:collection=>@tags),params[:search])  %>
    <%else%>
      <%= highlight(render(:partial=>"tags/tag_info" ,:collection=>@tags ,:locals=>{:type=>@tag_type}),params[:search])  %>
    <%end%>
    <%= will_paginate  @tags %>
  </div>
</div>
<div id="right_content">
  <%= link_to "<< 返回[#{@label}]首页" ,[ @owner ||  @tag_type.to_s.downcase.pluralize] %>
</div>