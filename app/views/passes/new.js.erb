<%  remote_form_for(Pass.new,:url=> user_resume_passes_path(params[:user_id],params[:resume_id]),
  :loading=>"$('pass_submit').disabled=true;",
  :complete=>"$('pass_submit').disabled=false;",
  :html=>{:method=>:post,:class=>"form_info required-validate"}) do |f| %>

  <%= render :partial=>"passes/form" ,:object=>f %>
  <%= f.submit '保存',:class=>"button" %>     <%= link_to_function "取消" ,"Element.hide('new_pass_container');"%>
<% end %>
