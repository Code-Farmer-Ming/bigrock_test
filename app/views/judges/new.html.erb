
<div class="form_info">
  <h1>创建评价</h1>

  <%  form_for(@judge,:url=> user_resume_pass_judges_path(params[:user_id],params[:resume_id],params[:pass_id]),
    :html=>{:method=>:post,:class=>"required-validate"}) do |f| %>
    <%= f.error_messages %>
    <%= render :partial=>"judges/form" ,:object=>f%>
    <%=  %>
    <p>
      <%= f.submit '保存',:disable_with=>"保存…",:disabled => false %>
    </p>
  <% end %>
</div>
