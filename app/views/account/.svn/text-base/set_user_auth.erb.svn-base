<%= render :partial=>"account/setting_types" %>
 
<div id="left_content">
  <div class="form_info">

    <% form_for(@user.setting ,:url=>set_user_auth_account_path()) do |form| %>
      <p>
        <%= form.label :apply_friend_auth,"添加好友",:class=>"filed_lab" %>
        <%= form.select :apply_friend_auth,UserSetting::APPLY_FRIEND_TYPES  %>
      </p>

      <p id="form_button">
        <%= form.submit "保存" %>
      </p>

    <%end%>
  </div>
</div>
<div id="right_content">

</div>