<div class="form_info">
    <h1>取回密码</h1>
    <% form_tag  forget_password_account_path() do %>

      <p>
        <%=label_tag  "email_label", "注册邮箱", {:for=>"email",:class=>"filed_lab"} %>
        <%= text_field_tag   "email",params[:email] ,:style=>"width:260px;",:maxlength=>64%>
        <%= submit_tag "发送"  %>
      <p>
      <%end%>
  </div>
