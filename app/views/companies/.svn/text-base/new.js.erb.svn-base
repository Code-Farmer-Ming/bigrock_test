<div class="form_info">
  <h1>创建新公司</h1>
  <%  remote_form_for(@company,:url => companies_path() ,:html => { :multipart => true }) do |f| %>
    <%= f.error_messages %>
    <%= render :partial=>"companies/form",:object=>f %>
    <p >
      <%= f.submit '保存' %> <%= link_to_close_lightbox "取消" %>
    </p>
  <% end %>
</div>
